{
    "name": "workflow",
    "private": true,
    "version": "0.0.1",
    "engines": {
        "node": ">=20.0.0 <21 || >=22.0.0 <23 || >=24.0.0 <25"
    },
    "main": "src/workflow.js",
    "bin": {
        "workflow": "src/workflow.js"
    },
    "scripts": {
        "postinstall": "run-script-os",
        "postinstall:win32": "if not exist .env copy .env.example .env else echo \"[INFO]: '.env' already exists!\"",
        "postinstall:darwin:linux": "[ ! -f .env ] && cp .env.example .env || echo \"[INFO]: '.env' already exists!\"",
        "workflow": "tsx src/workflow.ts"
    },
    "type": "module",
    "devDependencies": {
        "@eslint/js": "^9.39.0",
        "@types/debug": "^4.1.12",
        "@types/node": "^24.9.2",
        "eslint": "^9.39.0",
        "globals": "^16.4.0",
        "run-script-os": "^1.1.6",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@srhenry/type-utils": "https://github.com/SrHenry/type-utils/releases/download/v0.5.6/srhenry-type-utils-0.5.6.tgz",
        "chalk": "^5.6.2",
        "commander": "^14.0.2",
        "debug": "^4.4.3",
        "dotenv": "^17.2.3",
        "tsx": "^4.20.6"
    }
}
